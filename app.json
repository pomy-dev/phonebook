{
  "expo": {
    "name": "Business Link",
    "slug": "Business-Link",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pomy.phonebook",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSPhotoLibraryAddUsageDescription": "Allow this app to save images to your photo library.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app needs your location to show directions to the business."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCk4R94IPXuKkXvSm7RIr5fWlmIRzr6UA0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.pld.phonebook",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCk4R94IPXuKkXvSm7RIr5fWlmIRzr6UA0"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/adaptive-icon.png",
          "color": "#003366",
          "sounds": [],
          "mode": "production",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "03dcce6c-eb8e-4ede-8abe-12c1b2fcd575"
      }
    }
  }
}